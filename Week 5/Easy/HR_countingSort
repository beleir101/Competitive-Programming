public static List<Integer> countingSort(List<Integer> arr) {
    // Write your code here
        List<Integer> sorted = new ArrayList<Integer>();
        for (int i = 0; i < 100; i++){
            sorted.add(0);
        }
        for (int k : arr){
            sorted.set(k, sorted.get(k) + 1);
        }
        //System.out.println(sorted);
        return sorted;
    }
